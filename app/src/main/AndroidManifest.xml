<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.runey">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SecondView">
            <intent-filter>
                <action android:name="com.example.runey.SecondView" />

                <category android:name="android.intent.category.Default" />
            </intent-filter>
        </activity>
        <activity android:name=".Competition">
            <intent-filter>
                <action android:name="com.example.runey.Competition" />

                <category android:name="android.intent.category.Default" />
            </intent-filter>
        </activity>
        <activity android:name=".Parametrs">
            <intent-filter>
                <action android:name="com.example.runey.Parametrs" />

                <category android:name="android.intent.category.Default" />
            </intent-filter>
        </activity>
        <activity android:name=".MyRuney">
            <intent-filter>
                <action android:name="com.example.runey.MyRuney" />

                <category android:name="android.intent.category.Default" />
            </intent-filter>
        </activity>
        <activity android:name=".ChallengeWindow">
            <intent-filter>
                <action android:name="com.example.runey.SecondView" />

                <category android:name="android.intent.category.Default" />
            </intent-filter>
        </activity>
    </application>

</manifest>